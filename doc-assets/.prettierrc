{
	"useTabs": false,
	"printWidth": 180,
	"bracketSameLine": true,
	"bracketSpacing": true,
	"tabWidth": 2,
	"singleQuote": true,
	"overrides": [
		{
			"files": "*.html",
			"options": { "useTabs": false, "tabWidth": 2 }
		},
		{
			"files": "*.css",
			"options": { "useTabs": false, "tabWidth": 4 }
		}
	]
}
